{
  "name": "mapleland",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\"",
    "eslint": "eslint \"app/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint  \"app/**/*.{js,jsx,ts,tsx}\" --fix",
    "update-types": "npx supabase gen types typescript --linked --schema maple_land > app/_types/supabase.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.6",
    "clsx": "^2.1.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/gtag.js": "^0.0.18",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
